<%= render(SmallContainerComponent.new) do %>
  <% flash.each do |key, value| %>
    <%= content_tag :div, value, class: "alert alert-#{key}" %>
  <% end %>

  <% if current_user.essay_proposal.nil? %>
    <%= render "unregistered" %>
  <% else %>
    <%= render "registered" %>
  <% end %>
<% end %>
